<!DOCTYPE taglib
  PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
  "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">


<taglib>

  <!-- ============== Tag Library Description Elements ============= -->
  <tlib-version>0.03</tlib-version>
  <jsp-version>1.2</jsp-version>
  <short-name>JavaServerFaces Framework Tag Library</short-name>
  <uri>imageMaptaglibrary_1.1</uri>
  <description>
    This tag library contains tags for representing an imagemap.
  </description>

  <!-- ===================== ImageMap tags ====================== -->
  <tag>
    <name>image</name>
    <tag-class>cardemo.ImageTag</tag-class>
      <attribute>
        <name>id</name>
	<required>true</required>
	<rtexprvalue>false</rtexprvalue>
      </attribute>
      <attribute>
        <name>url</name>
	<required>true</required>
	<rtexprvalue>false</rtexprvalue>
      </attribute>
      <attribute>
        <name>usemap</name>
	<required>true</required>
	<rtexprvalue>false</rtexprvalue>
      </attribute>
  </tag>
 <tag>
    <name>map</name>
    <tag-class>cardemo.MapTag</tag-class>
      <attribute>
        <name>id</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
      </attribute>
      <attribute>
        <name>currentArea</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
      </attribute>
  </tag>
  <tag>
    <name>area</name>
    <tag-class>cardemo.AreaTag</tag-class>
      <attribute>
        <name>id</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
      </attribute>
      <attribute>
        <name>modelReference</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
      </attribute>
     <attribute>
      <name>onclick</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>onmouseover</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <name>onmouseout</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>    
  </tag>
</taglib>
