<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>


  <!-- =================== Initialize Image Map Hotspot Data ============== -->

  <managed-bean>
    <description>
      The image map hot spot for North America.
    </description>
    <managed-bean-name> NA </managed-bean-name>
    <managed-bean-class> components.model.ImageArea </managed-bean-class>
    <managed-bean-scope> application </managed-bean-scope>
    <managed-property>
      <property-name>shape</property-name>
      <value>poly</value>
    </managed-property>
    <managed-property>
      <property-name>alt</property-name>
      <value>NAmerica</value>
    </managed-property>
    <managed-property>
      <property-name>coords</property-name>
      <value>
        6,15,6,28,2,30,6,34,13,28,17,28,25,35,25,44,37,45,45,46,45,48,48,49,48,44,60,35,55,21,48,16,6,15
      </value>
    </managed-property>
  </managed-bean>

