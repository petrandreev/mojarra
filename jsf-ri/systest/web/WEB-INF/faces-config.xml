<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
 "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
 "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>


  <!-- Managed Bean #1, No Property Overrides -->
  <managed-bean>
    <managed-bean-name>test1</managed-bean-name>
    <managed-bean-class>
      com.sun.faces.systest.model.TestBean
    </managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>


  <!-- Managed Bean #2, Primitive Property Overrides -->
  <managed-bean>
    <managed-bean-name>test2</managed-bean-name>
    <managed-bean-class>
      com.sun.faces.systest.model.TestBean
    </managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>booleanProperty</property-name>
      <value>false</value>
    </managed-property>
    <managed-property>
      <property-name>byteProperty</property-name>
      <value>21</value>
    </managed-property>
    <managed-property>
      <property-name>doubleProperty</property-name>
      <value>321.54</value>
    </managed-property>
    <managed-property>
      <property-name>floatProperty</property-name>
      <value>21.43</value>
    </managed-property>
    <managed-property>
      <property-name>intProperty</property-name>
      <value>321</value>
    </managed-property>
    <managed-property>
      <property-name>longProperty</property-name>
      <value>54321</value>
    </managed-property>
    <managed-property>
      <property-name>shortProperty</property-name>
      <value>4321</value>
    </managed-property>
    <managed-property>
      <property-name>stringProperty</property-name>
      <value>New String Value</value>
    </managed-property>
    <managed-property>
      <property-name>userName</property-name>
      <value-ref>usernamecomponent</value-ref>
    </managed-property>
  </managed-bean>
 
   
  
  <!-- Message Resources Custom Bundle #1 - Single Element -->
  <message-resources>
    <message-resources-id>javax.faces.context.FACES_API_MESSAGES</message-resources-id>
    <message-resources-class>com.sun.faces.application.MessageResourcesImpl</message-resources-class>
    <message>
      <message-id>javax.faces.validator.DoubleRangeValidator.LIMIT</message-id>
      <message-class>javax.faces.application.MessageImpl</message-class>
      <summary xml:lang="en">Validation Error:This summary replaces the RI summary</summary>
    </message>
  </message-resources>

  <message-resources>
    <message-resources-id>Custom1</message-resources-id>
    <message>
      <message-id>Custom1A</message-id>
      <summary>This Is Custom1A Summary</summary>
      <detail>This Is Custom1A Detail</detail>
    </message>
    <message>
      <message-id>Custom1B</message-id>
      <summary>This Is Custom1B Summary</summary>
      <detail>This Is Custom1B Detail</detail>
    </message>
    <message>
      <message-id>Custom1B</message-id>
      <summary>This Is Custom1B Summary</summary>
      <detail>This Is Custom1B Detail</detail>
    </message>
    <message>
      <message-id>Custom1B</message-id>
      <summary xml:lang="en">This Is Custom1B Summary (en)</summary>
      <detail xml:lang="en">This Is Custom1B Detail (en)</detail>
    </message>
    <message>
      <message-id>Custom1B</message-id>
      <summary xml:lang="fr">This Is Custom1B Summary (fr)</summary>
      <detail xml:lang="fr">This Is Custom1B Detail (fr)</detail>
    </message>
    <message>
      <message-id>Custom1C</message-id>
      <summary>This Is Custom1C Summary</summary>
      <detail>This Is Custom1C Detail</detail>
    </message>
  </message-resources>

  <!-- Message Resources Custom Bundle #2 - Double Element -->
  <message-resources>
    <message-resources-id>Custom2</message-resources-id>
    <message>
      <message-id>Custom2A</message-id>
      <summary>This Is Custom2A Summary</summary>
      <detail>This Is Custom2A Detail</detail>
    </message>
    <message>
      <message-id>Custom2B</message-id>
      <summary>This Is Custom2B Summary</summary>
      <detail>This Is Custom2B Detail</detail>
    </message>
  </message-resources>

  <message-resources>
    <message-resources-id>Custom2</message-resources-id>
    <message>
      <message-id>Custom2B</message-id>
      <summary>This Is Custom2B Summary</summary>
      <detail>This Is Custom2B Detail</detail>
    </message>
    <message>
      <message-id>Custom2B</message-id>
      <summary xml:lang="en">This Is Custom2B Summary (en)</summary>
      <summary xml:lang="fr">This Is Custom2B Summary (fr)</summary>
      <detail xml:lang="en">This Is Custom2B Detail (en)</detail>
      <detail xml:lang="fr">This Is Custom2B Detail (fr)</detail>
    </message>
    <message>
      <message-id>Custom2C</message-id>
      <summary>This Is Custom2C Summary</summary>
      <detail>This Is Custom2C Detail</detail>
    </message>
  </message-resources>
  
   <!-- validator -->
  <validator>
    <description>TestValidator Description</description>
    <validator-id>TestValidator</validator-id>
    <validator-class>com.sun.faces.systest.TestValidator</validator-class>
  </validator>

  <!-- converter -->
  <converter>
    <description>Test Converter</description>
    <converter-id>TestConverter</converter-id>
    <converter-class>com.sun.faces.systest.TestConverter</converter-class>
  </converter>
  
  <!-- component -->
  <component>
    <component-type>TestComponent</component-type>
    <component-class>com.sun.faces.systest.TestComponent</component-class>
  </component>
  
   <managed-bean>
      <managed-bean-name>usernamecomponent</managed-bean-name>
      <managed-bean-class>
         javax.faces.component.base.UIInputBase
      </managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
      <managed-property>
        <property-name>size</property-name>
        <value>16</value>
      </managed-property>
      <managed-property>
        <property-name>maxlength</property-name>
        <value>32</value>
      </managed-property>
  </managed-bean>

  <!-- standard renderkit -->
  <render-kit>
    <render-kit-id>CUSTOM</render-kit-id>
    <render-kit-class>com.sun.faces.renderkit.RenderKitImpl</render-kit-class>
    <renderer>
      <renderer-type>Text</renderer-type>
      <renderer-class>com.sun.faces.systest.render.TextRenderer</renderer-class>
    </renderer>
  </render-kit>

  <!-- Component that renders its own "header" and "footer" facets -->
  <component>
    <component-type>FacetComponent</component-type>
    <component-class>com.sun.faces.systest.FacetComponent</component-class>
  </component>

  <!-- Component that renders its own children -->
  <component>
    <component-type>ChildrenComponent</component-type>
    <component-class>com.sun.faces.systest.ChildrenComponent</component-class>
  </component>


</faces-config>
