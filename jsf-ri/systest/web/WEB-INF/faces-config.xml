<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
 "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
 "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>


  <!-- Managed Bean #1, No Property Overrides -->
  <managed-bean>
    <managed-bean-name>test1</managed-bean-name>
    <managed-bean-class>
      com.sun.faces.systest.model.TestBean
    </managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>


  <!-- Managed Bean #2, Primitive Property Overrides -->
  <managed-bean>
    <managed-bean-name>test2</managed-bean-name>
    <managed-bean-class>
      com.sun.faces.systest.model.TestBean
    </managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>booleanProperty</property-name>
      <value>false</value>
    </managed-property>
    <managed-property>
      <property-name>byteProperty</property-name>
      <value>21</value>
    </managed-property>
    <managed-property>
      <property-name>doubleProperty</property-name>
      <value>321.54</value>
    </managed-property>
    <managed-property>
      <property-name>floatProperty</property-name>
      <value>21.43</value>
    </managed-property>
    <managed-property>
      <property-name>intProperty</property-name>
      <value>321</value>
    </managed-property>
    <managed-property>
      <property-name>longProperty</property-name>
      <value>54321</value>
    </managed-property>
    <managed-property>
      <property-name>shortProperty</property-name>
      <value>4321</value>
    </managed-property>
    <managed-property>
      <property-name>stringProperty</property-name>
      <value>New String Value</value>
    </managed-property>
  </managed-bean>


  <!-- Message Resources Custom Bundle #1 - Single Element -->
  <message-resources>
    <message-resources-id>Custom1</message-resources-id>
    <message>
      <message-id>Custom1A</message-id>
      <summary>This Is Custom1A Summary</summary>
      <detail>This Is Custom1A Detail</summary>
    </message>
    <message>
      <message-id>Custom1B</message-id>
      <summary>This Is Custom1B Summary</summary>
      <detail>This Is Custom1B Detail</summary>
    </message>
    <message>
      <message-id>Custom1B</message-id>
      <summary>This Is Custom1B Summary</summary>
      <detail>This Is Custom1B Detail</summary>
    </message>
    <message xml:lang="en">
      <message-id>Custom1B</message-id>
      <summary>This Is Custom1B Summary (en)</summary>
      <detail>This Is Custom1B Detail (en)</summary>
    </message>
    <message xml:lang="fr">
      <message-id>Custom1B</message-id>
      <summary>This Is Custom1B Summary (fr)</summary>
      <detail>This Is Custom1B Detail (fr)</summary>
    </message>
    <message>
      <message-id>Custom1C</message-id>
      <summary>This Is Custom1C Summary</summary>
      <detail>This Is Custom1C Detail</summary>
    </message>
  </message-resources>

  <!-- Message Resources Custom Bundle #2 - Double Element -->
  <message-resources>
    <message-resources-id>Custom2</message-resources-id>
    <message>
      <message-id>Custom2A</message-id>
      <summary>This Is Custom2A Summary</summary>
      <detail>This Is Custom2A Detail</summary>
    </message>
    <message>
      <message-id>Custom2B</message-id>
      <summary>This Is Custom2B Summary</summary>
      <detail>This Is Custom2B Detail</summary>
    </message>
  <message-resources>

  <message-resources>
    <message-resources-id>Custom2</message-resources-id>
    <message>
      <message-id>Custom2B</message-id>
      <summary>This Is Custom2B Summary</summary>
      <detail>This Is Custom2B Detail</summary>
    </message>
    <message xml:lang="en">
      <message-id>Custom2B</message-id>
      <summary>This Is Custom2B Summary (en)</summary>
      <detail>This Is Custom2B Detail (en)</summary>
    </message>
    <message xml:lang="fr">
      <message-id>Custom2B</message-id>
      <summary>This Is Custom2B Summary (fr)</summary>
      <detail>This Is Custom2B Detail (fr)</summary>
    </message>
    <message>
      <message-id>Custom2C</message-id>
      <summary>This Is Custom2C Summary</summary>
      <detail>This Is Custom2C Detail</summary>
    </message>
  </message-resources>

</faces-config>
