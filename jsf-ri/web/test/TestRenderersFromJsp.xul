<?xml version="1.0"?> <!-- -*- Mode: HTML -*- --> 

<page>

<page-url>/TestRenderersFromJsp.jsp</page-url>

<window id="basicForm" formName="basicForm" rendererType="FormRenderer">

<label id="userLabel" value="UserName" rendererType="TextRenderer" />
<textbox id="userName" value="default" rendererType="InputRenderer" />

<label id="pwdLabel" value="Password" rendererType="TextRenderer" />
<textbox id="password" type="password" rendererType="InputRenderer" />

<label id="addrLabel" value="Address" rendererType="TextRenderer" />
<textbox id="address" multiline="true" rendererType="TextAreaRenderer" />

<checkbox id="validUser" label="Checker" 
          modelReference="${TestBean.boolProp}" 
          rendererType="CheckboxRenderer" />

<radiogroup id="shipType" modelReference="${TestBean.prop}" 
            rendererType="RadioRenderer" >

<radio value="nextDay" label="Next Day" />
<radio value="nextWeek" label="Next Week" checked="true" />
<radio value="nextMonth" label="Next Month" />
</radiogroup>

<menupopup id="appleQuantity" modelReference="${TestBean.prop}" 
           rendererType="ListboxRenderer" >

<menuitem value="0" label="0.00"/>
<menuitem value="1" label="1.00"/>
<menuitem value="2" label="2.00"/>
<menuitem value="3" label="3.00"/>
<menuitem value="4" selected="true" label="4.00"/>
<menuitem value="5" label="5.00"/>
<menuitem value="6" label="6.00"/>
<menuitem value="7" label="7.00"/>
<menuitem value="8" label="8.00"/>
<menuitem value="9" label="9.00"/>

</menupopup>

<button id="login" label="Login" value="loginButton" 
        rendererType="ButtonRenderer" />

<button id="login2" src="duke.gif" value="loginButton" 
        rendererType="ButtonRenderer" />

</window>

</page>
