  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config>

  <application>

    <locale-config>
      <!-- no default-locale, should be the same as the system one -->
      <supported-locales>
        <locale>ps_PS</locale> <!-- Palestinians -->
        <locale>tg-AF</locale> <!-- Tajiks in Afghanistan -->
        <locale>tk_IQ-Traditional</locale> <!-- Turkmens in Iraq -->
        <locale>en_US</locale> <!-- Americans in U.S. -->
      </supported-locales>
    </locale-config>

  </application>

</faces-config>
