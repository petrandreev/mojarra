        <display-name></display-name>
        <icon></icon>
        <property-name>onkeypress</property-name>
        <property-class>java.lang.String</property-class>
      </property>

      <property>
        <description>Javascript code that is executed when a key was pressed down</description>
        <display-name></display-name>
        <icon></icon>
        <property-name>onkeydown</property-name>
        <property-class>java.lang.String</property-class>
      </property>

      <property>
        <description>Javascript code that is executed when a key was released</description>
        <display-name></display-name>
        <icon></icon>
        <property-name>onkeyup</property-name>
        <property-class>java.lang.String</property-class>
      </property>

