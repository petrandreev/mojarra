<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xmlns:xi="http://www.w3.org/2001/XInclude" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd" 
              version="1.2">
    
    <application>
        <message-bundle>com.sun.faces.sandbox.Messages</message-bundle>
    </application>
    
    <lifecycle>
        <phase-listener>com.sun.faces.sandbox.util.MultiFileUploadPhaseListener</phase-listener>
        <phase-listener>com.sun.faces.sandbox.util.DownloadPhaseListener</phase-listener>
    </lifecycle>
    <component>
        <component-type>com.sun.faces.sandbox.SelectItems</component-type>
        <component-class>com.sun.faces.sandbox.component.UISelectItems</component-class>
    </component>
    
    <component>
        <component-type>com.sun.faces.sandbox.AccessibleHtmlColumn</component-type>
        <component-class>com.sun.faces.sandbox.component.AccessibleHtmlColumn</component-class>
    </component>
    <component>
        <component-type>com.sun.faces.sandbox.AccessibleHtmlDataTable</component-type>
        <component-class>com.sun.faces.sandbox.component.AccessibleHtmlDataTable</component-class>
    </component>
    <render-kit>
        <render-kit-id>HTML_BASIC</render-kit-id>
        <render-kit-class>com.sun.faces.renderkit.RenderKitImpl</render-kit-class>
        <renderer>
            <component-family>javax.faces.Data</component-family>
            <renderer-type>com.sun.faces.sandbox.AccessibleTableRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.AccessibleTableRenderer</renderer-class>
        </renderer>
    </render-kit>
    
    
    <component>
        <component-type>com.sun.faces.sandbox.YuiTree</component-type>
        <component-class>com.sun.faces.sandbox.component.YuiTree</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.YuiTreeRenderer</renderer-type>
        </component-extension>
    </component>
    <render-kit>
        <renderer>
            <component-family>com.sun.faces.sandbox.YuiTree</component-family>
            <renderer-type>com.sun.faces.sandbox.YuiTreeRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.YuiTreeRenderer</renderer-class>
        </renderer>
    </render-kit>
    <component>
        <component-type>com.sun.faces.sandbox.YuiTreeNode</component-type>
        <component-class>com.sun.faces.sandbox.component.YuiTreeNode</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.YuiTreeNodeRenderer</renderer-type>
        </component-extension>
    </component>
    <render-kit>
        <renderer>
            <component-family>com.sun.faces.sandbox.YuiTreeNode</component-family>
            <renderer-type>com.sun.faces.sandbox.YuiTreeNodeRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.YuiTreeNodeRenderer</renderer-class>
        </renderer>
    </render-kit>
    
    <!-- *************************** YUI Calendar *************************** -->
    <component>                                                                                
        <component-type>com.sun.faces.sandbox.YuiCalendar</component-type>
        <component-class>com.sun.faces.sandbox.component.YuiCalendar</component-class>
        <property>
            <description>
                Determines whether to hide extra weeks that are completely outside the current month.
            </description>
            <display-name>Hide Blank Weeks</display-name>
            <icon />
            <property-name>hideBlankWeeks</property-name>
            <property-class>boolean</property-class>
            <property-extension>
                <value-expression-enabled>true</value-expression-enabled>
            </property-extension>
        </property>
        <property>
            <description>
                Determines whether the Calendar should allow for the selection of multiple dates.
            </description>
            <display-name>Multi-Select</display-name>
            <icon />
            <property-name>multiSelect</property-name>
            <property-class>boolean</property-class>
            <property-extension>
                <value-expression-enabled>true</value-expression-enabled>
            </property-extension>
        </property>
        <property>
            <description>
                Allows the user to wire in a custom on change event handler.
            </description>
            <display-name>On Change Event Handler</display-name>
            <icon />
            <property-name>onChange</property-name>
            <property-class>String</property-class>
            <property-extension>
                <value-expression-enabled>true</value-expression-enabled>
            </property-extension>
        </property>
        <property>
            <description>
                Determines whether to display the weekday headers.
            </description>
            <display-name>Show Weekdays</display-name>
            <icon />
            <property-name>showWeekdays</property-name>
            <property-class>boolean</property-class>
            <property-extension>
                <value-expression-enabled>true</value-expression-enabled>
            </property-extension>
        </property>
        <property>
            <description>
                Determines whether to display row footers.
            </description>
            <display-name>Show Week Footer</display-name>
            <icon />
            <property-name>showWeekFooter</property-name>
            <property-class>boolean</property-class>
            <property-extension>
                <value-expression-enabled>true</value-expression-enabled>
            </property-extension>
        </property>
        <property>
            <description>
                Determines whether to display row headers.
            </description>
            <display-name>Show Week Header</display-name>
            <icon />
            <property-name>showWeekHeader</property-name>
            <property-class>boolean</property-class>
            <property-extension>
                <value-expression-enabled>true</value-expression-enabled>
            </property-extension>
        </property>
        <property>
            <description>
                0-6, representing the day that a week begins on.
            </description>
            <display-name>Start Weekday</display-name>
            <icon />
            <property-name>startWeekday</property-name>
            <property-class>int</property-class>
            <property-extension>
                <value-expression-enabled>true</value-expression-enabled>
            </property-extension>
        </property>
        
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.YuiCalendarRenderer</renderer-type>
        </component-extension>                                                                                
    </component>
    
    <render-kit>
        <renderer>
            <component-family>com.sun.faces.sandbox.YuiCalendar</component-family>
            <renderer-type>com.sun.faces.sandbox.YuiCalendarRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.YuiCalendarRenderer</renderer-class>
        </renderer>    
    </render-kit>
    
    <!-- *************************** YUI Menu *************************** -->
    <component>
        <component-type>com.sun.faces.sandbox.YuiMenu</component-type>
        <component-class>com.sun.faces.sandbox.component.YuiMenu</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.YuiMenuRenderer</renderer-type>
        </component-extension>                                                                                
    </component>
    
    <render-kit>
        <renderer>
            <component-family>com.sun.faces.sandbox.YuiMenu</component-family>
            <renderer-type>com.sun.faces.sandbox.YuiMenuRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.YuiMenuRenderer</renderer-class>
        </renderer>    
    </render-kit>
    
    <component>
        <component-type>com.sun.faces.sandbox.YuiMenuItem</component-type>
        <component-class>com.sun.faces.sandbox.component.YuiMenuItem</component-class>
    </component>
    
    <!-- *************************** YUI Context Menu *************************** -->
    <component>
        <component-type>com.sun.faces.sandbox.YuiContextMenu</component-type>
        <component-class>com.sun.faces.sandbox.component.YuiContextMenu</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.YuiContextMenuRenderer</renderer-type>
        </component-extension>                                                                                
    </component>
    
    <render-kit>
        <renderer>
            <component-family>com.sun.faces.sandbox.YuiContextMenu</component-family>
            <renderer-type>com.sun.faces.sandbox.YuiContextMenuRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.YuiContextMenuRenderer</renderer-class>
        </renderer>    
    </render-kit>
    
    <!-- *************************** YUI Menu Bar *************************** -->
    <component>
        <component-type>com.sun.faces.sandbox.YuiMenuBar</component-type>
        <component-class>com.sun.faces.sandbox.component.YuiMenuBar</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.YuiMenuBarRenderer</renderer-type>
        </component-extension>                                                                                
    </component>
    
    <render-kit>
        <renderer>
            <component-family>com.sun.faces.sandbox.YuiMenuBar</component-family>
            <renderer-type>com.sun.faces.sandbox.YuiMenuBarRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.YuiMenuBarRenderer</renderer-class>
        </renderer>    
    </render-kit>
    <component>
        <description></description>
        <display-name>Download</display-name>
        <component-type>com.sun.faces.sandbox.FileDownload</component-type>
        <component-class>com.sun.faces.sandbox.component.FileDownload</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.FileDownloadRenderer</renderer-type>
        </component-extension>
    </component>
    
    <render-kit>
        <description>Renderkit implementation for the Download component</description>
        <renderer>
            <component-family>com.sun.faces.sandbox.FileDownload</component-family>
            <renderer-type>com.sun.faces.sandbox.FileDownloadRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.FileDownloadRenderer</renderer-class>
        </renderer>
    </render-kit>
    
    <component>
        <description></description>
        <display-name>MultiFileUpload</display-name>
        <component-type>com.sun.faces.sandbox.MultiFileUpload</component-type>
        <component-class>com.sun.faces.sandbox.component.MultiFileUpload</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.MultiFileUploadRenderer</renderer-type>
        </component-extension>
    </component>
    
    <render-kit>
        <description>Renderkit implementation for the MultiFileUpload component</description>
        <renderer>
            <component-family>com.sun.faces.sandbox.MultiFileUpload</component-family>
            <renderer-type>com.sun.faces.sandbox.MultiFileUploadRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.MultiFileUploadRenderer</renderer-class>
        </renderer>
    </render-kit>
    
    <component>
        <description></description>
        <display-name>TabView</display-name>
        <component-type>com.sun.faces.sandbox.YuiTabView</component-type>
        <component-class>com.sun.faces.sandbox.component.YuiTabView</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.YuiTabViewRenderer</renderer-type>
        </component-extension>
    </component>
    
    <render-kit>
        <description>Renderkit implementation for the TabView component</description>
        <renderer>
            <component-family>com.sun.faces.sandbox.YuiTabView</component-family>
            <renderer-type>com.sun.faces.sandbox.YuiTabViewRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.YuiTabViewRenderer</renderer-class>
        </renderer>
    </render-kit>
    
    <component>
        <description></description>
        <display-name>Tab</display-name>
        <component-type>com.sun.faces.sandbox.YuiTab</component-type>
        <component-class>com.sun.faces.sandbox.component.YuiTab</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.YuiTabRenderer</renderer-type>
        </component-extension>
    </component>
    
    <render-kit>
        <description>Renderkit implementation for the Tab component</description>
        <renderer>
            <component-family>com.sun.faces.sandbox.YuiTab</component-family>
            <renderer-type>com.sun.faces.sandbox.YuiTabRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.YuiTabRenderer</renderer-class>
        </renderer>
    </render-kit>

    <component>
        <description></description>
        <display-name>Html Editor</display-name>
        <component-type>com.sun.faces.sandbox.HtmlEditor</component-type>
        <component-class>com.sun.faces.sandbox.component.HtmlEditor</component-class>
        <component-extension>
            <renderer-type>com.sun.faces.sandbox.HtmlEditorRenderer</renderer-type>
        </component-extension>
    </component>
    
    <render-kit>
        <description>Renderkit implementation for the HtmlEditor component</description>
        <renderer>
            <component-family>com.sun.faces.sandbox.HtmlEditor</component-family>
            <renderer-type>com.sun.faces.sandbox.HtmlEditorRenderer</renderer-type>
            <renderer-class>com.sun.faces.sandbox.render.HtmlEditorRenderer</renderer-class>
        </renderer>
    </render-kit>
</faces-config>